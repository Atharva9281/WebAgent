# Agent B Configuration

# Gemini API Key (required)
# Get from: https://ai.google.dev/
GEMINI_API_KEY=your_api_key_here

# Browser Configuration
BROWSER_HEADLESS=false          # Keep browser visible during automation
SCREENSHOT_DELAY=1000           # Wait time after actions (milliseconds)

# Task Configuration  
MAX_STEPS_PER_TASK=15           # Maximum steps before timeout
RETRY_FAILED_ACTIONS=true       # Retry if action fails

# Dataset Configuration
DATASET_DIR=dataset             # Output directory for screenshots
COMPRESS_SCREENSHOTS=false      # Compress PNG images to save space

# Debug Options
VERBOSE_LOGGING=true            # Detailed console output
SAVE_ANNOTATED_IMAGES=true      # Save images with bounding boxes
